{
  "name": "CeloSendPOS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build": "cd android && gradlew assembleRelease && cd ..",
    "clean": "cd android && gradlew clean && cd ..",
    "deploy": "cd android && gradlew assembleRelease && adb install -r app/build/outputs/apk/release/app-release.apk",
    "ios": "react-native run-ios",
    "installrn": "yarn add -D rn-nodeify",
    "installapp": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "lint": "eslint .",
    "postinstall": "node_modules/.bin/rn-nodeify --install assert,buffer,events,crypto,stream,http,https,url,path,fs --hack --yarn",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.7.0",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@tradle/react-native-http": "2.0.1",
    "@walletconnect/client": "^1.8.0",
    "abi-decoder": "^2.4.0",
    "assert": "2.0.0",
    "asyncstorage-down": "^4.2.0",
    "aws-iot-device-sdk": "^2.2.12",
    "aws-sdk": "^2.1206.0",
    "axios": "^1.1.3",
    "bip39": "^3.0.4",
    "buffer": "6.0.3",
    "events": "^3.3.0",
    "hdkey": "^2.0.1",
    "https-browserify": "~1.0.0",
    "path-browserify": "1.0.1",
    "react": "18.2.0",
